   "cell_type": "code",
   "execution_count": 17,
   "id": "f4c80c02",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[('34587', 163.8), ('98762', 284.0), ('77226', 108.85000000000001), ('88112', 84.97)]\n"
     ]
    }
   ],
   "source": [
    "## Solución del estudiante 1 - Eduardo Gutiérrez Flores\n",
    "orders_est1 = [[\"34587\", \"Learning Python, Mark Lutz\", 4, 40.95], \n",
    "              [\"98762\", \"Programming Python, Mark Lutz\", 5, 56.80], \n",
    "              [\"77226\", \"Head First Python, Paul Barry\", 3,32.95],\n",
    "              [\"88112\", \"Einführung in Python3, Bernd Klein\", 3, 24.99]]\n",
    "\n",
    "tuplas = list(map(lambda x: (x[0], x[2] * x[3] + 10 if x[2] * x[3] < 100 else x[2] * x[3]), orders_est1))\n",
    "\n",
    "print(tuplas)"
   ]
  },
